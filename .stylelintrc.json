{
  "extends": [
    "stylelint-config-standard"
  ],
  "rules": {
    "declaration-empty-line-before": [
      "never",
      {
        "ignore": [
          "after-declaration"
        ]
      }
    ],
    "custom-property-empty-line-before": null,
    "selector-class-pattern": [
      "",
      {}
    ],
    "value-keyword-case": null
  }
}